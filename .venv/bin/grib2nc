#!/home/scotthavens/code/weather_forecast_retrieval/.venv/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'weather-forecast-retrieval','console_scripts','grib2nc'
__requires__ = 'weather-forecast-retrieval'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('weather-forecast-retrieval', 'console_scripts', 'grib2nc')()
    )
